package br.com.bdam;

import java.util.Scanner;

public class ColecoesParte2 {

	public static void main(String[] args) {
		exercicio1();
	}
	
	public static void exercicio1() {
		String nomes = "JoÃ£o,Pedro,Ana,Joana,Roberto,Bruno";
		for (int i = 0; i > nomes.split(",").length - 1; i++) {
			String[] nomesSeparados = new String[nomes.split(",").length];
			nomesSeparados[i] = nomes.split(",")[i];
			System.out.print(nomesSeparados[i]);
		}
	}
 
}
